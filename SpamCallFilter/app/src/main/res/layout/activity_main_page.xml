<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.indiana.lofields.spamcallfilter.MainPageActivity">

    <ImageButton
        android:id="@+id/iconPlaceHolder1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="0"
        android:layout_columnWeight="1"
        android:layout_row="1"
        android:layout_rowWeight="1"
        android:contentDescription="@string/iconHolderDescrip"
        app:srcCompat="@android:drawable/sym_action_call" />

    <ImageButton
        android:id="@+id/iconPlaceHolder2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_columnWeight="1"
        android:layout_row="1"
        android:layout_rowWeight="1"
        android:contentDescription="@string/iconHolderDescrip"
        app:srcCompat="@android:drawable/presence_busy" />

    <ImageButton
        android:id="@+id/iconPlaceHolder3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="0"
        android:layout_columnWeight="1"
        android:layout_row="2"
        android:layout_rowWeight="1"
        android:contentDescription="@string/iconHolderDescrip"
        app:srcCompat="@android:drawable/ic_input_add" />

    <ImageButton
        android:id="@+id/iconPlaceHolder4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:layout_columnWeight="1"
        android:layout_row="2"
        android:layout_rowWeight="1"
        android:contentDescription="@string/iconHolderDescrip"
        app:srcCompat="@android:drawable/ic_input_add" />

</GridLayout>
